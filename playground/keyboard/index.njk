{% include 'head.njk'%}
{% include 'header.njk' %}

	<section class="page__header">
		<div class="container">
			<h1>JS Keyboard</h1>
		</div>
	</section>

	<main class="page__main main--js-keyboard">
		<div class="container">
			<div class="js-keybaord">
				<ul class="js-keyboard__row js-keyboard__row-0">
					<li class="js-keyboard__key pinky mobile" id="esc">ESC</li>
					<li class="js-keyboard__key pinky" id="1">1</li>
					<li class="js-keyboard__key ring" id="2">2</li>
					<li class="js-keyboard__key middle" id="3">3</li>
					<li class="js-keyboard__key pointer1st" id="4">4</li>
					<li class="js-keyboard__key pointer2nd" id="5">5</li>
					<li class="js-keyboard__key pointer2nd" id="6">6</li>
					<li class="js-keyboard__key pointer1st" id="7">7</li>
					<li class="js-keyboard__key middle" id="8">8</li>
					<li class="js-keyboard__key ring" id="9">9</li>
					<li class="js-keyboard__key pinky" id="10">0</li>
					<li class="js-keyboard__key pinky mobile">-</li>
					<li class="js-keyboard__key pinky">+</li>
					<li class="js-keyboard__key pinky mobile" id="back">BACK</li>
				</ul>
				<ul class="js-keyboard__row js-keyboard__row-1">
					<li class="js-keyboard__key pinky mobile" id="tab">TAB</li>
					<li class="js-keyboard__key pinky" id="Q">Q</li>
					<li class="js-keyboard__key ring" id="W">W</li>
					<li class="js-keyboard__key middle" id="E">E</li>
					<li class="js-keyboard__key pointer1st" id="R">R</li>
					<li class="js-keyboard__key pointer2nd" id="T">T</li>
					<li class="js-keyboard__key pointer2nd" id="Y">Y</li>
					<li class="js-keyboard__key pointer1st" id="U">U</li>
					<li class="js-keyboard__key middle" id="I">I</li>
					<li class="js-keyboard__key ring" id="O">O</li>
					<li class="js-keyboard__key pinky" id="P">P</li>
					<li class="js-keyboard__key pinky mobile">[</li>
					<li class="js-keyboard__key pinky mobile">]</li>
					<li class="js-keyboard__key pinky mobile">\</li>
				</ul>
				<ul class="js-keyboard__row js-keyboard__row-2">
					<li class="js-keyboard__key pinky mobile" id="caps">CAPS</li>
					<li class="js-keyboard__key pinky" id="A">A</li>
					<li class="js-keyboard__key ring" id="S">S</li>
					<li class="js-keyboard__key middle" id="D">D</li>
					<li class="js-keyboard__key pointer1st" id="F">F</li>
					<li class="js-keyboard__key pointer2nd" id="G">G</li>
					<li class="js-keyboard__key pointer2nd" id="H">H</li>
					<li class="js-keyboard__key pointer1st" id="J">J</li>
					<li class="js-keyboard__key middle" id="K">K</li>
					<li class="js-keyboard__key ring" id="L">L</li>
					<li class="js-keyboard__key pinky mobile">:</li>
					<li class="js-keyboard__key pinky mobile">''</li>
					<li class="js-keyboard__key pinky mobile" id="enter">ENTER</li>
				</ul>
				<ul class="js-keyboard__row js-keyboard__row-3">
					<li class="js-keyboard__key pinky mobile" id="left-shift">SHIFT</li>
					<li class="js-keyboard__key pinky" id="Z">Z</li>
					<li class="js-keyboard__key ring" id="X">X</li>
					<li class="js-keyboard__key middle" id="C">C</li>
					<li class="js-keyboard__key pointer1st" id="V">V</li>
					<li class="js-keyboard__key pointer2nd" id="B">B</li>
					<li class="js-keyboard__key pointer2nd" id="N">N</li>
					<li class="js-keyboard__key pointer1st" id="M">M</li>
					<li class="js-keyboard__key middle">,</li>
					<li class="js-keyboard__key ring">.</li>
					<li class="js-keyboard__key pinky ">;</li>
					<li class="js-keyboard__key pinky mobile" id="right-shift">SHIFT</li>
				</ul>
			</div>
		</div>
	</main>

{% include 'footer.njk' %}